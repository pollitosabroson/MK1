#vista{:pagina => "inicio"} 
  %ul
    - @fotos.each do |foto|
      %li.thumb
        = link_to image_tag(foto.pic_large, :width => 400, :height => 400),ver_path(foto), :"data-id" => foto.instagram_id
        %p
          = link_to "Picture by: @#{foto.username}", foto.instagram_link, :target => 'blank'
