.vista{:pagina => "inicio"} 
  %ul
    - @fotos.each do |foto|
      %li.thumb
        %h1= link_to "Picture by: @#{foto.username}", foto.instagram_link, :target => 'blank'

        = link_to image_tag(foto.pic_large, :width => "90%", :height => "90%"),ver_path(foto), :"data-id" => foto.instagram_id